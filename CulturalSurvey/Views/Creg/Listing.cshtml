    .buttons-pdf {
        background-color: #E91E63;
        color: white;
    }

    .buttons-excel {
        background-color: #FF5722;
        color: white;
    }


    .navbar-default .navbar-toggle {
        border-color: transparent;
        color: #fff !important;
    }
